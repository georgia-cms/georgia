<tr class="page <%= page.state %>">
  <td>
    <%= page.title %>
  </td>
  <td>
    <%= page.template %>
  </td>
  <td>
    <%= page.url %>
  </td>
  <td>
    <%= page.tag_list %>
  </td>
  <td class="labels">
    <%= page.status_tag %>
  </td>
  <td class="actions">
    <%= link_to_group_edit page, publish: true, review: true, delete: true, copy: true, preview: true %>
  </td>
</tr>
