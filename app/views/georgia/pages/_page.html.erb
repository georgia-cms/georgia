<tr id='<%= dom_id(page) %>' class="page">
  <td class='checkbox-cell'>
    <%= checkboxable_tag page %>
  </td>
  <td>
    <%= icon_tag 'pencil hide pull-right muted' %>
    <%= link_to page.title, [:edit, page], class: 'page-title js-toggle-icon' %>
  </td>
  <td>
    <%= page_tag_list(page) %>
    <span class='muted'><%= page.url %></span>
  </td>
  <td>
    <%= page.template %>
  </td>
  <td class='timestamp'>
    <em><%= page.updated_at.strftime('%F') %></em>
  </td>
</tr>